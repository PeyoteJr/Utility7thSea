<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="utility7thsea.controller.PresetController">

    <HBox layoutX="280.0" layoutY="589.0" prefHeight="111.0" prefWidth="681.0" spacing="80.0">
        <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="147.0" prefWidth="300.0" text="INDIETRO" />
            <Button fx:id="createButton" mnemonicParsing="false" onAction="#onCreateButtonClick" prefHeight="147.0" prefWidth="300.0" text="CREA PRESET" />
        </children>
    </HBox>
    <TableView fx:id="presetTable" layoutX="40.0" layoutY="101.0" prefHeight="488.0" prefWidth="1200.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="charactersColumn" prefWidth="75.0" text="Personaggi" />
            <TableColumn fx:id="operationColumn" prefWidth="75.0" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
    <Text layoutX="543.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="PRESET">
        <font>
            <Font size="48.0" />
        </font>
    </Text>

</AnchorPane>
